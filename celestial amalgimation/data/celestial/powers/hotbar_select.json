{
	"name": "many powers",
	"description": "each slot is linked to a diffrent ability use secondary key to toggle them",
	
	"type": "origins:multiple",
	"one_selected": {
		"type": "origins:action_over_time",
		"interval": 1,
		"rising_action": {
			"type": "origins:and",
			"actions": [
				{
					"type": "origins:execute_command",
					"command": "title @s actionbar {\"text\":\"Possession\",\"color\":\"gold\"}",
					"permission_level": 2
				}
			]
		},
		"condition": {
			"type": "origins:and",
			"conditions": [
				{
					"type": "origins:command",
					"command": "data get entity @s SelectedItemSlot",
					"comparison": "==",
					"compare_to": 0
				},
				{
					"type": "origins:command",
					"command": "execute if entity @s[tag=NoTwin]",
					"comparison": "==",
					"compare_to": 0
				}
			]
		}
	},
	
